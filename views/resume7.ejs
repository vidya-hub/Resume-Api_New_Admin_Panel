<body>
    <div class="container">
        <div style="border:3px solid #000;">
            <div class="top_main">
                <div class="top_sub">
                    <div class="name">
                        <%= firstName %> <%= lastName %>
                    </div>
                    <div class="post">
                        <!-- BUSINESS ANALYSt -->
