<!doctype html>
<html moznomarginboxes mozdisallowselectionprint>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=0.5, maximum-scale=3.0">
    <title>PDF 1</title>
    <style type="text/css" media="print">
        @page {
            size: auto;
            margin: 5mm;
        }
        html{zoom: 0.7;}

        /* uncomment for check A4 size */
         @page {
            size: 7in 9.25in;
            margin: 27mm 16mm 27mm 16mm;
        } 

        @media print {
            .footer,
            #non-printable {
                display: none !important;
            }
            #printable {
                display: block;
            }
        }
    </style>
</head>

<body style="padding:0px; margin:0px; box-sizing: border-box;">
    <div style=" margin:0px 10px; padding:0px;font-family: PT Sans, sans-serif;">
        <div>
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
                <tr>
                    <td>
                        <table width="100%" border="0" cellpadding="0" cellspacing="0">
                            <tr>
                                <table width="100%" border="0" cellpadding="0" cellspacing="0"
                                    style="padding-top: 30px;">
                                    <tr>
                                        <td width="35%" valign="top"
                                            style="padding: 30px; background: #f2d8d6;     word-break: break-word;">
                                            <p
                                                style="margin:0; margin-bottom: 30px; font-size: 30px; line-height: 30px; color: grey;">
                                                <%= firstName %> <%= lastName %></p>

                                            <p style="margin:0; margin-bottom: 10px;"><i
                                                    style="background: #fff; width: 20px; height: 20px; line-height: 20px; border-radius: 50%; margin-right: 10px; font-size: 10px; display: inline-block;text-align: center; color: grey; ">@</i><span
                                                    style="display: inline-block; vertical-align: top; width: calc(100% - 35px);"><%=email %>
                                            </p>
                                            <p style="margin:0; margin-bottom: 10px;"><i
                                                    style="background: #fff; width: 20px; height: 20px; line-height: 20px; border-radius: 50%; margin-right: 10px; font-size: 10px; display: inline-block;text-align: center; color: grey; ">h</i><span
                                                    style="display: inline-block; vertical-align: top; width: calc(100% - 35px);"><%=phoneNo %>
                                            </p>
                                            <p style="margin:0; margin-bottom: 10px;"><i
                                                    style="background: #fff; width: 20px; height: 20px; line-height: 20px; border-radius: 50%; margin-right: 10px; font-size: 10px; display: inline-block;text-align: center; color: grey; ">a</i><span
                                                    style="display: inline-block; vertical-align: top; width: calc(100% - 35px);"><%=address %>
                                            </p>
                                           
                                        </td>
                                    </tr>

                                </table>
                            </tr>

                        </table>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</body>

</html>